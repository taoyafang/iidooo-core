<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="orange-mobile" namespace="/mobile" extends="iidooo-default">
		<interceptors>
			<interceptor name="userGUIDInterceptor" class="com.pingan.orange.interceptor.UserGUIDInterceptor"></interceptor>
		</interceptors>
		<action name="index" class="com.pingan.orange.action.mobile.IndexAction">
			<interceptor-ref name="userGUIDInterceptor"></interceptor-ref>
			<result name="success">/WEB-INF/pages/mobile/index.html</result>
		</action>
		<action name="init" class="com.pingan.orange.action.mobile.IndexAction" method="init">
			<interceptor-ref name="userGUIDInterceptor"></interceptor-ref>
		</action>
		<action name="roll" class="com.pingan.orange.action.mobile.IndexAction" method="roll">
			<interceptor-ref name="userGUIDInterceptor"></interceptor-ref>
		</action>	
	</package>
</struts> 